<NotepadPlus>
    <UserLang name="TupiLight" ext="tp th" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="no" foldCompact="yes" forcePureLC="0" decimalSeparator="0" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">00$ 01 02((EOL)) 03($ 04$)</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
            <Keywords name="Operators1">&amp; + - * / = , . ; { } ( ) [ ]</Keywords>
            <Keywords name="Operators2">ref</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open"></Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close"></Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
            <Keywords name="Keywords1">fn&#x000D;&#x000A;struct&#x000D;&#x000A;cstruct&#x000D;&#x000A;union&#x000D;&#x000A;typedef</Keywords>
            <Keywords name="Keywords2">usefn&#x000D;&#x000A;useth&#x000D;&#x000A;usetp&#x000D;&#x000A;uselib</Keywords>
            <Keywords name="Keywords3">i8&#x000D;&#x000A;i16&#x000D;&#x000A;i32&#x000D;&#x000A;i64&#x000D;&#x000A;iptr&#x000D;&#x000A;f32&#x000D;&#x000A;f64&#x000D;&#x000A;byte&#x000D;&#x000A;word&#x000D;&#x000A;dword&#x000D;&#x000A;qword&#x000D;&#x000A;real4&#x000D;&#x000A;real8</Keywords>
            <Keywords name="Keywords4">return&#x000D;&#x000A;mark&#x000D;&#x000A;goto&#x000D;&#x000A;if&#x000D;&#x000A;elseif&#x000D;&#x000A;else&#x000D;&#x000A;loop&#x000D;&#x000A;while&#x000D;&#x000A;break&#x000D;&#x000A;continue</Keywords>
            <Keywords name="Keywords5">sizeof&#x000D;&#x000A;lengthof&#x000D;&#x000A;length&#x000D;&#x000A;size&#x000D;&#x000A;offset</Keywords>
            <Keywords name="Keywords6">TRUE&#x000D;&#x000A;FALSE&#x000D;&#x000A;NULL</Keywords>
            <Keywords name="Keywords7">?</Keywords>
            <Keywords name="Keywords8"></Keywords>
            <Keywords name="Delimiters">00# 01 02((EOL)) 03 04 05 06&apos; 07 08((EOL &apos;)) 09 10 11 12&quot; 13 14((EOL &quot;)) 15 16 17 18&lt; 19 20((EOL &gt;)) 21 22 23</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="008040" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="LINE COMMENTS" fgColor="008040" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="0000FF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="FF8040" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="008040" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="FF8000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="804000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="FF0000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS7" fgColor="800000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS8" fgColor="000000" bgColor="FFFFFF" fontStyle="0" nesting="0" />
            <WordsStyle name="OPERATORS" fgColor="808000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
